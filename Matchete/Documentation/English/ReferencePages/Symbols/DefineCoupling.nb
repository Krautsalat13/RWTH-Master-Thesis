 RowBox[{"DefineCoupling", "[", 
  RowBox[{"Cee", ",", 
   RowBox[{"Indices", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"Flavor", ",", "Flavor", ",", "Flavor", ",", "Flavor"}], "}"}]}],
    ",", 
   RowBox[{"Symmetries", "->", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"SymmetricPermutation", "[", 
       RowBox[{"3", ",", "4", ",", "1", ",", "2"}], "]"}], ",", 
      RowBox[{"SymmetricPermutation", "[", 
       RowBox[{"1", ",", "4", ",", "3", ",", "2"}], "]"}]}], "}"}]}]}], 
  "]"}]}], "Input",
