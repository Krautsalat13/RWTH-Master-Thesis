Cell["\<\
The basis for the \[ScriptD]-dimensional Dirac algebra is infinite \
dimensional. A suitable basis is formed by all anti-symmetrized products of \
the gamma matrices. We use the notation \
\>", "TechNoteText",
 CellID->546840777,ExpressionUUID->"20185cc4-ba15-4cdf-bdc4-fd714c3d723a"],

Cell[TextData[{
 "\[RawEscape]",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["\[CapitalGamma]", 
    RowBox[{
     SubscriptBox["\[Mu]", "1"], 
     SubscriptBox["\[Ellipsis]\[Mu]", "n"]}]], "=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"n", "!"}]], 
    RowBox[{
     UnderscriptBox["\[Sum]", "\[Sigma]"], 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "\[Sigma]"], " ", 
      SubscriptBox["\[Gamma]", 
       SubscriptBox["\[Mu]", 
        RowBox[{"\[Sigma]", 
         RowBox[{"(", "1", ")"}]}]]], 
      SubscriptBox["\[Ellipsis]\[Gamma]", 
       SubscriptBox["\[Mu]", 
        RowBox[{"\[Sigma]", 
         RowBox[{"(", "n", ")"}]}]]]}]}]}]}]], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "c180635b-8d0f-4c73-82e5-f11b2442996e"]
}], "DisplayMath",
 CellID->1938153869,ExpressionUUID->"e2d34472-74c1-436f-94ce-50a158c0ce53"],

