 RowBox[{"DefineCG", "[", 
  RowBox[{"t3", ",", 
   RowBox[{"{", 
    RowBox[{"adj", ",", "fund", ",", 
     RowBox[{"Bar", "@", "fund"}]}], "}"}], ",", 
   RowBox[{"Generators", "[", 
    RowBox[{
     RowBox[{"SU", "@", "3"}], ",", 
     RowBox[{"{", 
      RowBox[{"1", ",", "0"}], "}"}]}], "]"}]}], "]"}]], "Input",
